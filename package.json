  "repository": {
    "type": "git",
    "url": "git+https://github.com/mustaquenadim/mustaquenadim.github.io.git"
  },
  "keywords": [
    "nextjs"
  ],
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
