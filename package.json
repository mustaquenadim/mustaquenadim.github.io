    "react-dom": "^18.2.0",
    "react-fast-marquee": "^1.6.4",
    "react-icons": "^5.0.1",
    "react-text-transition": "^3.1.0",
    "react-transition-group": "^4.4.5",
    "remark": "^15.0.1",
    "remark-html": "^16.0.1",
    "remark-prism": "^1.3.6",
    "scrollreveal": "^4.0.9",
    "styled-components": "^6.1.8",
    "swiper": "^11.0.7"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.0",
